    /* opacity: 0.5; */
  }
  .more-icon{
    width:32rpx;
    height:32rpx;
    cursor: pointer;
    -webkit-tap-highlight-color: transparent;
  }
  .more-popup{
    box-sizing: border-box;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    background-color: #ffffff;
    padding: 16rpx 32rpx;
    border-radius: 0 0 24rpx 24rpx;
    overflow: hidden;
    flex-wrap: wrap;
    gap: 24rpx;
    z-index: 9999;
    transform: translateY(-100%); /* 元素初始位置位于顶部之外 */
    opacity: 0; /* 初始透明度为 0 */
    transition: transform 0.5s ease-in-out, opacity 0.5s ease-in-out; 
  }
  .more-popup-active{
    transform: translateY(0); /* 元素滑入到正常位置 */
    opacity: 1; /* 透明度变为 1 */
  }

